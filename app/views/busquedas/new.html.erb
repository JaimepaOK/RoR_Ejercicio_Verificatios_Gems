<h1><%= t('avanzed_sh') %></h1>

<%= form_with model: @busquedas do |f| %>
    
    <div class="field">
    <%= f.label :motor %>
    <%= f.text_field :motor %>
  </div>

  <div class="field">
    <%= f.label :marca %>
    <%= f.text_field :marca %>
  </div>

  <div class="field">
    <%= f.label :color %>
    <%= f.text_field :color %>
  </div>

    <%= f.submit t("Search") %>
<% end %>

<p><%= link_to t("Back"), vehiculos_path %></p>